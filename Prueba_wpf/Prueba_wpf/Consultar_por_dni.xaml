        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Prueba_wpf"
        mc:Ignorable="d"
        Title="Consultar_por_dni" Height="216" Width="585.5">
    <Grid>
        <Label x:Name="label1" Content="Introduzca el DNI de la persona que desee ver" HorizontalAlignment="Left" Margin="86,54,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txt_box_dni" HorizontalAlignment="Left" Height="22" Margin="164,92,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btn_buscar_por_dni" Content="Buscar" HorizontalAlignment="Left" Margin="321,92,0,0" VerticalAlignment="Top" Width="76" Click="btn_iniciar_busqueda_Click"/>
        <Button x:Name="btn_cerrar" Content="Cerrar" HorizontalAlignment="Left" Margin="253,147,0,0" VerticalAlignment="Top" Width="75" Click="btn_cerrar_Click"/>

    </Grid>
</Window>
